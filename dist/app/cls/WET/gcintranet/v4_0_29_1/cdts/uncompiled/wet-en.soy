		<![endif]-->
		<script src='{call .environment data="all" /}js/theme.min.js'></script>
		{if $exitScript == true and isNonnull($exitURL)}
		<script data-id="exitScript" data-token="{$exitURL}" {if isNonnull($exitDomains)}data-token-domains="{$exitDomains}" {/if}{if $displayModal == false}data-token-modal="false" {/if}src='{call .environment data="all" /}cdts/exitScript.js'></script>
		<a id="sExitModalLink" class="wb-lbx lbx-modal wb-inv" title="" href="#sExitModal">Secure exit notice</a>
		<section id="sExitModal" class="mfp-hide modal-dialog modal-content overlay-def">
			<header class="modal-header">
				<h2 class="modal-title">Warning</h2>
			</header>
			<div class="modal-body">
				<p>
					{if isNonnull($exitMsg)}
					{$exitMsg}
					{else}
					You are about to leave a secure site, do you wish to continue?
					{/if}
				</p>
				<a href="#" type="button" class="btn btn-primary popup-modal-dismiss pull-left" id="eCancel"><span class="glyphicon glyphicon-arrow-left"></span>&nbsp;Cancel</a>
				<a href="#" type="button" class="btn btn-default popup-modal pull-right" id="eOK">Yes&nbsp;<span class="glyphicon glyphicon-arrow-right"></span></a>
				<br />&nbsp;
			</div>
		</section>
		{/if}
{/template}

