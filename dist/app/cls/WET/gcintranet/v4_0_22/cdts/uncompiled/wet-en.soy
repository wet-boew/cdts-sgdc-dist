/**
 * This is all of the html to be included after existing page content.
 * @param? dateModified The date the content was last modified.
 * @param? showPostContent Used to determine if the postcontent space will be displayed, off by default.
 * @param? versionIdentifier Used instead of dateModified
 * @param? screenIdentifier Used to identify screens for users that need help
 */
{template .preFooter}
		{if $showPostContent != false}
		{/if}
		<div class="row pagedetails">
			{if isNonnull($screenIdentifier)}
			<div class="col-xs-12 mrgn-bttm-md">
				<div class="well well-sm screenIdent">Screen Identifier:&#32;{$screenIdentifier}</div>
			</div>
			{/if}
			<div class="col-sm-5 col-xs-12 datemod">
				<dl id="wb-dtmd">
					{if isNonnull($dateModified)}
					<dt>Date modified:&#32;</dt>
					<dd><time property="dateModified">{$dateModified}</time></dd>
					{else}
					{if isNonnull($versionIdentifier)}
					<dt>Version:&#32;</dt>
					<dd>{$versionIdentifier}</dd>
					{/if}
					{/if}
				</dl>
			</div>
		</div>
{/template}

/**
 * This is all of the html for the secondary menu.
 * @param sections Loops of the sections
 */
{template .secmenu}
<h2 id="wb-sec-h" class="wb-inv">Section menu</h2>
{foreach $section in $sections}
<section class="list-group menu list-unstyled">
	<h3>{if isNonnull($section.sectionLink)}<a href="{$section.sectionLink}"{if $section.newWindow==true} target="_blank"{/if}>{/if}{$section.sectionName}{if $section.newWindow==true}<span class="wb-inv"> This link will open in a new window</span>{/if}{if isNonnull($section.sectionLink)}</a>{/if}</h3>
	<ul class="list-group menu list-unstyled">
		{foreach $link in $section.menuLinks}
		{if isNonnull($link.subLinks)}
		<li>
			<a href="{$link.href}" class="list-group-item"{if $link.newWindow==true} target="_blank"{/if}>{$link.text}{if $link.newWindow==true}<span class="wb-inv"> This link will open in a new window</span>{/if}</a>
			<ul class="list-group menu list-unstyled">
				{foreach $sublink in $link.subLinks}
				<li><a href="{$sublink.subhref}" class="list-group-item"{if $sublink.newWindow==true} target="_blank"{/if}>{$sublink.subtext}{if $sublink.newWindow==true}<span class="wb-inv"> This link will open in a new window</span>{/if}</a></li>
				{/foreach}
			</ul>
		</li>
		{else}
		<li><a href="{$link.href}" class="list-group-item"{if $link.newWindow==true} target="_blank"{/if}>{$link.text}{if $link.newWindow==true}<span class="wb-inv"> This link will open in a new window</span>{/if}</a></li>
		{/if}
		{/foreach}
	</ul>
</section>
{/foreach}
{/template}

