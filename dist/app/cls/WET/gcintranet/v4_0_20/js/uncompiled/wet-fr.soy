/**
 * This is all of the references to be made before existing page content.
 * @param? templateDomain The domain from which to get WET files. 
 * @param? cdnEnv Used to switch between qat, nonprod, and prod CDN implementations.
 */
{template .refTop}
	<!--[if gte IE 9 | !IE ]><!-->
	<link href="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_20/GCWeb/assets/favicon.ico" rel="icon" type="image/x-icon">
	<link rel="stylesheet" href="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_20/GCWeb/css/theme.min.css">
	<!--<![endif]-->
	<!--[if lt IE 9]>
	<link href="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_20/GCWeb/assets/favicon.ico" rel="shortcut icon" />
	<link rel="stylesheet" href="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_20/GCWeb/css/ie8-theme.min.css" />
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
	<script src="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_20/wet-boew/js/ie8-wet-boew.min.js"></script>
	<![endif]-->
{/template}
/**
 * This is all of the html to be included before existing page content.
 * @param? templateDomain The domain from which to get WET files. 
 * @param? cdnEnv Used to switch between qat, nonprod, and prod CDN implementations.
 * @param? search Used to display search.
 * @param? siteMenu Used to display site menu.
 * @param? lngLinks Links to pages in other languages. 
 * @param? showPreContent Used to determine if the precontent space will be displayed.
 * @param? breadcrumbs Used to set custom breadcrumbs.
 * @param? topSecMenu Set to true if you want to use the secondary navigation.
 */
{template .top}
	<ul id="wb-tphp">
		<li class="wb-slc">
			<a class="wb-sl" href="#wb-cont">Passer au contenu principal</a>
		</li>
		<li class="wb-slc visible-sm visible-md visible-lg">
			<a class="wb-sl" href="#wb-info">Passer à «&#160;À propos de ce site&#160;»</a>
		</li>
		{if $topSecMenu == true}
		<li class="wb-slc visible-md visible-lg">
			<a class="wb-sl" href="#wb-sec">Passer au menu de la section</a>
		</li>
		{/if}
	</ul>
	<header role="banner">
		<div id="wb-bnr" class="container">
			{if isNonnull($lngLinks)}
			<section id="wb-lng" class="visible-md visible-lg text-right">
				<h2 class="wb-inv">Sélection de la langue</h2>
				<div class="row">
					<div class="col-md-12">
						<ul class="list-inline margin-bottom-none">
							{foreach $link in $lngLinks}
							<li><a lang="{$link.lang}" href="{$link.href}">{$link.text}</a></li>
							{/foreach}
						</ul>
					</div>
				</div>
			</section>
			{/if}
			<div class="row">
				<div class="brand col-xs-8 col-sm-9 col-md-6">
					{if $siteMenu == false}<a href="http://www.canada.ca/fr/index.html">{/if}<object type="image/svg+xml" tabindex="-1" data="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_20/GCWeb/assets/sig-blk-fr.svg"></object><span class="wb-inv"> Gouvernement du Canada</span>{if $siteMenu == false}</a>{/if}
				</div>
				<section class="wb-mb-links col-xs-4 col-sm-3 visible-sm visible-xs" id="wb-glb-mn">
					<h2>Recherche et menus</h2>
					<ul class="list-inline text-right chvrn">
						<li><a href="#mb-pnl" title="Recherche et menus" aria-controls="mb-pnl" class="overlay-lnk" role="button"><span class="glyphicon glyphicon-search"><span class="glyphicon glyphicon-th-list"><span class="wb-inv">Recherche et menus</span></span></span></a></li>
					</ul>
					<div id="mb-pnl"></div>
				</section>
				{if $search != false}
				<section id="wb-srch" class="col-xs-6 text-right visible-md visible-lg">
					<h2>Recherche</h2>
					<form action="https://recherche-search.gc.ca/rGs/s_r?#wb-land" method="get" name="cse-search-box" role="search" class="form-inline">
						<div class="form-group">
							<label for="wb-srch-q" class="wb-inv">Recherchez le site Web</label>
							<input id="wb-srch-q" list="wb-srch-q-ac" class="wb-srch-q form-control" name="q" type="search" value="" size="27" maxlength="150" placeholder="Rechercher dans intranet.canada.ca">
							<datalist id="wb-srch-q-ac">
								<!--[if lte IE 9]><select><![endif]-->
								<!--[if lte IE 9]></select><![endif]-->
							</datalist>
						</div>
						<div class="form-group submit">
							<button type="submit" id="wb-srch-sub" class="btn btn-primary btn-small" name="wb-srch-sub"><span class="glyphicon-search glyphicon"></span><span class="wb-inv">Recherche</span></button>
						</div>
					</form>
				</section>
				{/if}
			</div>
		</div>
		{if $siteMenu != false}
		<nav role="navigation" id="wb-sm" data-ajax-fetch="{if isNonnull($templateDomain)}$templateDomain{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_20/ajax/sitemenu-fra.html" data-trgt="mb-pnl" class="wb-menu visible-md visible-lg" typeof="SiteNavigationElement">
			<div class="container nvbar">
				<h2>Menu des sujets</h2>
				<div class="row">
					<ul class="list-inline menu">
						<li><a href="http://intranet.canada.ca/hgw-cgf/index-fra.asp">Comment le gouvernement fonctionne</a></li>
						<li><a href="http://intranet.canada.ca/wg-tg/index-fra.asp">Travailler au GC</a></li>
						<li><a href="http://intranet.canada.ca/cdl-dca/index-fra.asp">Développement de carrière et apprentissage</a></li>
						<li><a href="http://intranet.canada.ca/tr-or/index-fra.asp">Outils et ressources</a></li>
					</ul>
				</div>
			</div>
		</nav>
		{/if}
		{if $breadcrumbs != false}
		<nav role="navigation" id="wb-bc" property="breadcrumb">
			<h2>Vous êtes ici :</h2>
			<div class="container">
				<div class="row">
					<ol class="breadcrumb">
						{if isNonnull($breadcrumbs)}
						{foreach $item in $breadcrumbs}
						<li>{if isNonnull($item.acronym)}<acronym title="{$item.acronym}">{/if}{if isNonnull($item.href)}<a href="{$item.href}">{/if}{$item.title}{if isNonnull($item.href)}</a>{/if}{if isNonnull($item.acronym)}</acronym>{/if}</li>
						{/foreach}
						{else}
						<li><a href="http://www.canada.ca/fr/index.html">Accueil</a></li>
						{/if}
					</ol>
				</div>
			</div>
		</nav>
		{/if}
	</header>
	{if $showPreContent != false}
	{/if}
{/template}

/**
 * This is all of the html to be included after existing page content.
 * @param dateModified The date the content was last modified.
 * @param? showPostContent Used to determine if the postcontent space will be displayed.
 * @param? showFeedback Used to determine if the feedback button will be displayed
 * @param? showShare Used to determine if the share button will be displayed
 * @param? versionIdentifier Used instead of dateModified
 * @param? identifierNumber Used to determine the identifier of the version
 */
{template .preFooter}
		{if $showPostContent != false}
		{/if}
		<div class="row pagedetails">
			<div class="col-sm-5 col-xs-12 datemod">
				<dl id="wb-dtmd">
					{if $versionIdentifier == true}
					<dt>Version&#160;:&#32;</dt>
					<dd>{$identifierNumber}</dd>
					{else}
					<dt>Date de modification&#160;:&#32;</dt>
					<dd><time property="dateModified">{$dateModified}</time></dd>
					{/if}
				</dl>
			</div>
			{if $showFeedback != false}
			<div class="clear visible-xs"></div>
			<div class="col-sm-4 col-xs-6">
				<a href="http://www.canada.ca/fr/contact/retroaction.html" class="btn btn-default"><span class="glyphicon glyphicon-comment mrgn-rght-sm"></span>Rétroaction<span class="wb-inv"> sur ce site Web</span></a>
			</div>
			{/if}
			{if $showShare != false}
			<div class="col-sm-3 col-xs-6 text-right">
				{if isNonnull($showShare)}
				<div class="wb-share" data-wb-share='{lb}"filter": [{foreach $item in $showShare}"{$item}"{if not isLast($item)}, {/if}{/foreach}], "lnkClass": "btn btn-default"{rb}'></div>
				{else}
				<div class="wb-share" data-wb-share='{lb}"lnkClass": "btn btn-default"{rb}'></div>
				{/if}
			</div>
			<div class="clear visible-xs"></div>
			{/if}
		</div>
{/template}

/**
 * This is all of the html for the secondary menu.
 * @param sections Loops of the sections
 */
{template .secmenu}
		<h2 id="wb-sec-h" class="wb-inv">Section menu</h2>
		{foreach $section in $sections}
		<ul class="list-group menu list-unstyled">
			<li>
				<h3 class="wb-navcurr">{$section.sectionName}</h3>
				<ul class="list-group menu list-unstyled">
					{foreach $link in $section.menuLinks}
						{if isNonnull($link.subLinks)}
							<li><a class="list-group-item" href="{$link.href}">{$link.text}</a>
								<ul class="list-group menu list-unstyled">
								{foreach $sublink in $link.subLinks}
									<li><a class="list-group-item" href="{$sublink.subhref}">{$sublink.subtext}</a></li>
								{/foreach}
								</ul>
							</li>
						{else}
							<li><a class="list-group-item" href="{$link.href}">{$link.text}</a></li>
						{/if}
					{/foreach}
				</ul>
			</li>
		</ul>
		{/foreach}
{/template}

/**
 * This is all of the html to be included after existing page content.
 * @param? templateDomain The domain from which to get WET files. 
