								<!--[if lte IE 9]><select><![endif]-->
								<!--[if lte IE 9]></select><![endif]-->
							</datalist>
						</div>
						<div class="form-group submit">
							<button type="submit" id="wb-srch-sub" class="btn btn-primary btn-small" name="wb-srch-sub"><span class="glyphicon-search glyphicon"></span><span class="wb-inv">Search</span></button>
						</div>
					</form>
				</section>
				{/if}
			</div>
		</div>
		{if $siteMenu != false}
		<nav role="navigation" id="wb-sm" data-ajax-fetch="{if isNonnull($templateDomain)}$templateDomain{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_20/ajax/sitemenu-eng.html" data-trgt="mb-pnl" class="wb-menu visible-md visible-lg" typeof="SiteNavigationElement">
