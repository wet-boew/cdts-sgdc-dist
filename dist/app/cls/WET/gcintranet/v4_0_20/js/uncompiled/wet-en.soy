	{if $showPreContent != false}
	{/if}
{/template}

/**
 * This is all of the html to be included after existing page content.
 * @param dateModified The date the content was last modified.
 * @param? showPostContent Used to determine if the postcontent space will be displayed, off by default.
 * @param? showFeedback Used to determine if the feedback button will be displayed
 * @param? showShare Used to determine if the share button will be displayed
 * @param? versionIdentifier Used instead of dateModified
 * @param? identifierNumber Used to determine the identifier of the version
 */
{template .preFooter}
		{if $showPostContent != false}
		{/if}
		<div class="row pagedetails">
			<div class="col-sm-5 col-xs-12 datemod">
				<dl id="wb-dtmd">
					{if $versionIdentifier == true}
					<dt>Version:&#32;</dt>
					<dd>{$identifierNumber}</dd>
					{else}
					<dt>Date modified:&#32;</dt>
					<dd><time property="dateModified">{$dateModified}</time></dd>
					{/if}
				</dl>
			</div>
			{if $showFeedback != false}
			<div class="clear visible-xs"></div>
			<div class="col-sm-4 col-xs-6">
				<a href="http://www.canada.ca/en/contact/feedback.html" class="btn btn-default"><span class="glyphicon glyphicon-comment mrgn-rght-sm"></span>Feedback<span class="wb-inv"> about this web site</span></a>
			</div>
			{/if}
			{if $showShare != false}
			<div class="col-sm-3 col-xs-6 text-right">
				{if isNonnull($showShare)}
				<div class="wb-share" data-wb-share='{lb}"filter": [{foreach $item in $showShare}"{$item}"{if not isLast($item)}, {/if}{/foreach}], "lnkClass": "btn btn-default"{rb}'></div>
				{else}
				<div class="wb-share" data-wb-share='{lb}"lnkClass": "btn btn-default"{rb}'></div>
				{/if}
			</div>
			<div class="clear visible-xs"></div>
			{/if}
		</div>
{/template}

/**
 * This is all of the html for the secondary menu.
 * @param sections Loops of the sections
 */
{template .secmenu}
		<h2 id="wb-sec-h" class="wb-inv">Section menu</h2>
		{foreach $section in $sections}
		<ul class="list-group menu list-unstyled">
			<li>
				<h3 class="wb-navcurr">{$section.sectionName}</h3>
				<ul class="list-group menu list-unstyled">
					{foreach $link in $section.menuLinks}
						{if isNonnull($link.subLinks)}
							<li><a class="list-group-item" href="{$link.href}">{$link.text}</a>
								<ul class="list-group menu list-unstyled">
								{foreach $sublink in $link.subLinks}
									<li><a class="list-group-item" href="{$sublink.subhref}">{$sublink.subtext}</a></li>
								{/foreach}
								</ul>
							</li>
						{else}
							<li><a class="list-group-item" href="{$link.href}">{$link.text}</a></li>
						{/if}
					{/foreach}
				</ul>
			</li>
		</ul>
		{/foreach}
{/template}

