/**
 * This is all of the html to be included before existing page content.
 * @param? templateDomain The domain from which to get WET files. 
 * @param? cdnEnv Used to switch between qat, nonprod, and prod CDN implementations.
 * @param? search Used to display search.
 * @param? siteMenu Used to display site menu.
 * @param? lngLinks Links to pages in other languages.
 * @param? showPreContent Used to determine if the precontent space will be displayed.
 * @param? breadcrumbs Used to set custom breadcrumbs.
 * @param? topSecMenu Set to true if you want to use the secondary navigation.
 */
{template .top}
	<ul id="wb-tphp">
		<li class="wb-slc"><a class="wb-sl" href="#wb-cont">Skip to main content</a></li>
		<li class="wb-slc visible-sm visible-md visible-lg"><a class="wb-sl" href="#wb-info">Skip to "About this site"</a></li>
		{if $topSecMenu == true}
		<li class="wb-slc visible-md visible-lg"><a class="wb-sl" href="#wb-sec">Skip to section menu</a></li>
		{/if}
	</ul>
	<header role="banner">
		<div id="wb-bnr" class="container">
			{if isNonnull($lngLinks)}
			<section id="wb-lng" class="visible-md visible-lg text-right">
				<h2 class="wb-inv">Language selection</h2>
				<div class="row">
					<div class="col-md-12">
						<ul class="list-inline margin-bottom-none">
							{foreach $link in $lngLinks}
								//{if isFirst($link)}
									<li><a lang="{$link.lang}" href="{$link.href}">{$link.text}</a></li>
								//{/if}
							{/foreach}
						</ul>
					</div>
				</div>
			</section>
			{/if}
			<div class="row">
				<div class="brand col-xs-8 col-sm-9 col-md-6">
					{if $siteMenu != false}<a href="http://intranet.canada.ca/index-eng.asp">{/if}<object type="image/svg+xml" tabindex="-1" data="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_21/assets/sig-blk-en.svg"></object><span class="wb-inv"> Government of Canada</span>{if $siteMenu != false}</a>{/if}
				</div>
				<section class="wb-mb-links col-xs-4 col-sm-3 visible-sm visible-xs" id="wb-glb-mn">
					<h2>Search and menus</h2>
					<ul class="list-inline text-right chvrn">
						<li><a href="#mb-pnl" title="Search and menus" aria-controls="mb-pnl" class="overlay-lnk" role="button"><span class="glyphicon glyphicon-search"><span class="glyphicon glyphicon-th-list"><span class="wb-inv">Search and menus</span></span></span></a></li>
					</ul>
					<div id="mb-pnl"></div>
				</section>
				{if $search != false}
				<section id="wb-srch" class="col-xs-6 text-right visible-md visible-lg">
					<h2>Search</h2>
					<form action="http://intranet.canada.ca/search-recherche/query-recherche-eng.aspx" method="get" name="cse-search-box" role="search" class="form-inline">
						<div class="form-group">
							<label for="wb-srch-q" class="wb-inv">Search website</label>
							<input class="wb-srch-q form-control" name="q"  value="" size="27" maxlength="150" placeholder="Search">
							<input type="hidden" name="a" value="s" />
							<input type="hidden" name="s" value="1" />
							<input type="hidden" name="chk2" value="True" />
						</div>
						<div class="form-group submit">
							<button type="submit" id="wb-srch-sub" class="btn btn-primary btn-small" name="wb-srch-sub"><span class="glyphicon-search glyphicon"></span><span class="wb-inv">Search</span></button>
						</div>
					</form>
				</section>
				{/if}
			</div>
		</div>
		<div id="app-brand">
			<div class="container">
				<div class="row">
					<div class="col-sm-3">
						<div class="app-name">
							<a href="http://intranet.canada.ca/index-eng.asp"><span><span class="bold-gc">GC</span>intranet</span></a>
						</div>
					</div>
					<div class="col-sm-6 col-sm-offset-3">
						<div class="pull-right tool-link">
							<a href="http://www.gcpedia.gc.ca/wiki/?setlang="><img class="tool-link-icon" src="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_21/assets/pedia_icon.png" alt="GCpedia" /><span class="bold-gc">GC</span>pedia</a>
						</div>
						<div class="pull-right tool-link">
							<a href="https://gcconnex.gc.ca"><img class="tool-link-icon" src="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_21/assets/connex_icon.png" alt="GCconnex"/><span class="bold-gc">GC</span>connex</a>
						</div>
					</div>
				</div>
			</div>
		</div>
		{if $siteMenu != false}
		<nav role="navigation" id="wb-sm" data-ajax-replace="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_21/ajax/sitemenu-eng.html" data-trgt="mb-pnl" class="wb-menu visible-md visible-lg" typeof="SiteNavigationElement">
			<div class="container nvbar">
				<h2>Topics menu</h2>
				<div class="row">
					<ul class="list-inline menu">
						<li><a href="http://intranet.canada.ca/hgw-cgf/index-eng.asp">How government works</a></li>
						<li><a href="http://intranet.canada.ca/wg-tg/index-eng.asp">Working at GC</a></li>
						<li><a href="http://intranet.canada.ca/cdl-dca/index-eng.asp">Career development and learning</a></li>
						<li><a href="http://intranet.canada.ca/tr-or/index-eng.asp">Tools and resources</a></li>
					</ul>
				</div>
			</div>
		</nav>
		{/if}
		{if $breadcrumbs != false}
		<nav role="navigation" id="wb-bc" property="breadcrumb">
			<h2>You are here:</h2>
			<div class="container">
				<div class="row">
					<ol class="breadcrumb">
						{if isNonnull($breadcrumbs)}
						{foreach $item in $breadcrumbs}
						<li>{if isNonnull($item.acronym)}<abbr title="{$item.acronym}">{/if}{if isNonnull($item.href)}<a href="{$item.href}">{/if}{$item.title}{if isNonnull($item.href)}</a>{/if}{if isNonnull($item.acronym)}</abbr>{/if}</li>
						{/foreach}
						{else}
						<li><a href="http://intranet.canada.ca/index-eng.asp">Home</a></li>
						{/if}
					</ol>
				</div>
			</div>
		</nav>
		{/if}
	</header>
	{if $showPreContent != false}
	{/if}
{/template}

/**
 * This is all of the html to be included after existing page content.
 * @param? dateModified The date the content was last modified.
 * @param? showPostContent Used to determine if the postcontent space will be displayed, off by default.
 * @param? versionIdentifier Used instead of dateModified
 */
{template .preFooter}
		{if $showPostContent != false}
		{/if}
		<div class="row pagedetails">
			<div class="col-sm-5 col-xs-12 datemod">
				<dl id="wb-dtmd">
					{if isNonnull($dateModified)}
					<dt>Date modified:&#32;</dt>
					<dd><time property="dateModified">{$dateModified}</time></dd>
					{else}
					{if isNonnull($versionIdentifier)}
					<dt>Version:&#32;</dt>
					<dd>{$versionIdentifier}</dd>
					{/if}
					{/if}
				</dl>
			</div>
		</div>
{/template}

/**
 * This is all of the html for the secondary menu.
 * @param sections Loops of the sections
 */
{template .secmenu}
<h2 id="wb-sec-h" class="wb-inv">Section menu</h2>
{foreach $section in $sections}
<section class="list-group menu list-unstyled">
	<h3>{if isNonnull($section.sectionLink)}<a href="{$section.sectionLink}">{/if}{$section.sectionName}{if isNonnull($section.sectionLink)}</a>{/if}</h3>
	<ul class="list-group menu list-unstyled">
		{foreach $link in $section.menuLinks}
		{if isNonnull($link.subLinks)}
		<li>
			<a href="{$link.href}" class="list-group-item">{$link.text}</a>
			<ul class="list-group menu list-unstyled">
				{foreach $sublink in $link.subLinks}
				<li><a href="{$sublink.subhref}" class="list-group-item">{$sublink.subtext}</a></li>
				{/foreach}
			</ul>
		</li>
		{else}
		<li><a href="{$link.href}" class="list-group-item">{$link.text}</a></li>
		{/if}
		{/foreach}
	</ul>
</section>
{/foreach}
{/template}

