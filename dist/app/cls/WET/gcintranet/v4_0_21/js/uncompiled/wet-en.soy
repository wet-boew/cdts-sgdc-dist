{template .splash}
		<div id="bg">
			<img src="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_21/img/splash/sp-bg-2.jpg" alt="">
		</div>
		<main role="main">
			<div class="sp-hb">
				<div class="sp-bx col-xs-12">
					<h1 property="name" class="wb-inv">GC Intranet / Intranet du GC</h1>
					<div class="row">
						<div class="col-xs-11 col-md-8">
							<object type="image/svg+xml" tabindex="-1" role="img" data="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_21/assets/sig-spl.svg" width="283" aria-label="Government of Canada / Gouvernement du Canada"></object>
						</div>
					</div>
					<div class="row">
						<section class="col-xs-6 text-right">
							{if isNonnull($nameEng)}
							<h2 style="min-height:4.5em;">{$nameEng}</h2>
							{else}
							<h2 class="wb-inv">Government of Canada</h2>
							{/if}
							<p><a href="{$indexEng}" class="btn btn-primary">English</a></p>
						</section>
						<section class="col-xs-6" lang="fr">
							{if isNonnull($nameFra)}
							<h2 style="min-height:4.5em;">{$nameFra}</h2>
							{else}
							<h2 class="wb-inv">Gouvernement du Canada</h2>
							{/if}
							<p><a href="{$indexFra}" class="btn btn-primary">Français</a></p>
						</section>
					</div>
				</div>
				<div class="sp-bx-bt col-xs-12">
					<div class="row">
						<div class="col-xs-7 col-md-8">
							<a href="{$termsEng}" class="sp-lk">Terms & conditions</a> <span class="glyphicon glyphicon-asterisk"></span> <a href="{$termsFra}" class="sp-lk" lang="fr">Avis</a>
						</div>
						<div class="col-xs-5 col-md-4 text-right mrgn-bttm-md">
							<object type="image/svg+xml" tabindex="-1" role="img" data="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_21/assets/wmms-spl.svg" width="127" aria-label="Symbol of the Government of Canada / Symbole du gouvernement du Canada"></object>
						</div>
					</div>
				</div>
			</div>
		</main>
		<!--[if gte IE 9 | !IE ]><!-->
		<script src="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat/app/cls/WET/gcintranet/v4_0_21/js{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv/app/cls/WET/gcintranet/v4_0_21/js{else}https://ssl-templates.services.gc.ca/app/cls/WET/gcintranet/v4_0_21/js{/if}/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
		<script src="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_21/js/wet-boew.min.js"></script>
		<!--<![endif]-->
		<!--[if lt IE 9]>
		<script src="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='qat'}https://cdn-canada.services.gc.qat{elseif $cdnEnv=='nonprod'}https://s2tst-cdn-canada.sade-edap.prv{else}https://ssl-templates.services.gc.ca{/if}/app/cls/WET/gcintranet/v4_0_21/js/ie8-wet-boew2.min.js"></script>
		<![endif]-->
{/template}