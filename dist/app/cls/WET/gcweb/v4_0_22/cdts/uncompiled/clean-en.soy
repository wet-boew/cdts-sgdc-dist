/**
 * This is all of the html to be included before existing page content.
 * @param? templateDomain The domain from which to get WET files. 
 * @param? cdnEnv Used to switch between qat, nonprod, and prod CDN implementations.
 * @param? search Used to display search.
 * @param? siteMenu Used to display site menu.
 * @param? lngLinks Links to pages in other languages.
 * @param? showPreContent Used to determine if the precontent space will be displayed.
 * @param? breadcrumbs Used to set custom breadcrumbs.
 * @param? topSecMenu Set to true if you want to use the secondary navigation.
 */
{template .top}
	<ul id="wb-tphp">
		<li class="wb-slc"><a class="wb-sl" href="#wb-cont">Skip to main content</a></li>
		<li class="wb-slc visible-sm visible-md visible-lg"><a class="wb-sl" href="#wb-info">Skip to "About this site"</a></li>
		{if $topSecMenu == true}
		<li class="wb-slc visible-md visible-lg"><a class="wb-sl" href="#wb-sec">Skip to section menu</a></li>
		{/if}
	</ul>
	<header role="banner">
		<div id="wb-bnr" class="container">
			{if isNonnull($lngLinks)}
			<section id="wb-lng" class="visible-md visible-lg text-right">
				<h2 class="wb-inv">Language selection</h2>
				<div class="row">
					<div class="col-md-12">
						<ul class="list-inline margin-bottom-none">
							{foreach $link in $lngLinks}
								//{if isFirst($link)}
									<li><a lang="{$link.lang}" href="{$link.href}">{$link.text|cleanHtml}</a></li>
								//{/if}
							{/foreach}
						</ul>
					</div>
				</div>
			</section>
			{/if}
			<div class="row">
				<div class="brand col-xs-8 col-sm-9 col-md-6">
					{if $siteMenu != false}<a href="https://www.canada.ca/en/index.html">{/if}<object type="image/svg+xml" tabindex="-1" data="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='esdcqat'}https://cdn-canada.services.gc.qat/app/cls/WET{elseif $cdnEnv=='esdcnonprod'}https://s2tst-cdn-canada.sade-edap.prv/app/cls/WET{elseif $cdnEnv=='esdcprod'}https://ssl-templates.services.gc.ca/app/cls/WET{else}https://www.canada.ca/etc/designs/canada/cdts{/if}/gcweb/v4_0_22/assets/sig-blk-en.svg"></object><span class="wb-inv"> Government of Canada</span>{if $siteMenu != false}</a>{/if}
				</div>
				<section class="wb-mb-links col-xs-4 col-sm-3 visible-sm visible-xs" id="wb-glb-mn">
					<h2>Search and menus</h2>
					<ul class="list-inline text-right chvrn">
						<li><a href="#mb-pnl" title="Search and menus" aria-controls="mb-pnl" class="overlay-lnk" role="button"><span class="glyphicon glyphicon-search"><span class="glyphicon glyphicon-th-list"><span class="wb-inv">Search and menus</span></span></span></a></li>
					</ul>
					<div id="mb-pnl"></div>
				</section>
				{if $search != false}
				<section id="wb-srch" class="col-xs-6 text-right visible-md visible-lg">
					<h2>Search</h2>
					<form action="https://recherche-search.gc.ca/rGs/s_r?#wb-land" method="get" name="cse-search-box" role="search" class="form-inline">
						<div class="form-group">
							<label for="wb-srch-q" class="wb-inv">Search website</label>
							<input name="cdn" value="canada" type="hidden">
							<input name="st" value="s" type="hidden">
							<input name="num" value="10" type="hidden">
							<input name="langs" value="en" type="hidden">
							<input name="st1rt" value="1" type="hidden">
							<input name="s5bm3ts21rch" value="x" type="hidden">
							<input id="wb-srch-q" list="wb-srch-q-ac" class="wb-srch-q form-control" name="q" type="search" value="" size="27" maxlength="150" placeholder="Search Canada.ca">
							<input type="hidden" name="_charset_" value="UTF-8"/>
							<datalist id="wb-srch-q-ac">
								<!--[if lte IE 9]><select><![endif]-->
								<!--[if lte IE 9]></select><![endif]-->
							</datalist>
						</div>
						<div class="form-group submit">
							<button type="submit" id="wb-srch-sub" class="btn btn-primary btn-small" name="wb-srch-sub"><span class="glyphicon-search glyphicon"></span><span class="wb-inv">Search</span></button>
						</div>
					</form>
				</section>
				{/if}
			</div>
		</div>
		{if $siteMenu != false}
		<nav role="navigation" id="wb-sm" data-ajax-replace="https://cdn.canada.ca/gcweb-cdn-{if $cdnEnv=='prod'}live{else}dev{/if}/sitemenu/sitemenu-en.html" data-trgt="mb-pnl" class="wb-menu visible-md visible-lg" typeof="SiteNavigationElement">
			<div class="container nvbar">
				<h2>Topics menu</h2>
				<div class="row">
					<ul class="list-inline menu">
						<li><a href="http://www.esdc.gc.ca/en/jobs/index.page">Jobs</a></li>
						<li><a href="http://www.cic.gc.ca/english/index.asp">Immigration</a></li>
						<li><a href="http://travel.gc.ca">Travel</a></li>
						<li><a href="https://www.canada.ca/en/services/business/index.html">Business</a></li>
						<li><a href="https://www.canada.ca/en/services/benefits/index.html">Benefits</a></li>
