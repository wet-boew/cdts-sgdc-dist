		</nav>
		{/if}
		{if $breadcrumbs != false}
		<nav role="navigation" id="wb-bc" property="breadcrumb">
			<h2>You are here:</h2>
			<div class="container">
				<div class="row">
					<ol class="breadcrumb">
						{if isNonnull($breadcrumbs)}
						{foreach $item in $breadcrumbs}
						<li>{if isNonnull($item.acronym)}<abbr title="{$item.acronym|cleanHtml}">{/if}{if isNonnull($item.href)}<a href="{$item.href|escapeUri}">{/if}{$item.title|cleanHtml}{if isNonnull($item.href)}</a>{/if}{if isNonnull($item.acronym)}</abbr>{/if}</li>
						{/foreach}
						{else}
						<li><a href="https://www.canada.ca/en/index.html">Home</a></li>
						{/if}
					</ol>
				</div>
			</div>
		</nav>
		{/if}
	</header>
	{if $showPreContent != false}
	{/if}
{/template}

/**
 * This is all of the html to be included after existing page content.
 * @param? dateModified The date the content was last modified.
 * @param? showPostContent Used to determine if the postcontent space will be displayed, off by default.
 * @param? showShare Used to determine if the share button will be displayed
 * @param? showFeedback Used to determine if the "Report a problem ..." button will be displayed or customized
 * @param? versionIdentifier Used instead of dateModified
 * @param? screenIdentifier Used to identify screens for users that need help
 */
{template .preFooter}
		{if $showPostContent != false}
		{/if}
		<div class="row pagedetails">
			{if isNonnull($showFeedback)}
				{if $showFeedback != false}
			<div class="col-sm-6 col-lg-4 mrgn-tp-sm">
				<a href="{$showFeedback|escapeUri}" class="btn btn-default btn-block">Report a problem or mistake on this page</a>
			</div>
				{/if}
			{else}
			<div class="col-sm-6 col-lg-4 mrgn-tp-sm">
				<a href="https://www.canada.ca/en/report-problem.html" class="btn btn-default btn-block">Report a problem or mistake on this page</a>
			</div>
			{/if}
			{if $showShare != false}
			<div class="col-sm-3 mrgn-tp-sm pull-right">
				{if isNonnull($showShare)}
				<div class="wb-share" data-wb-share='{lb}"filter": [{foreach $item in $showShare}"{$item}"{if not isLast($item)}, {/if}{/foreach}], "lnkClass": "btn btn-default btn-block"{rb}'></div>
				{else}
				<div class="wb-share" data-wb-share='{lb}"lnkClass": "btn btn-default btn-block"{rb}'></div>
				{/if}
			</div>
			{/if}
			{if isNonnull($screenIdentifier)}
			<div class="col-xs-12 mrgn-tp-lg">
				<div class="well well-sm screenIdent">Screen Identifier:&#32;{$screenIdentifier|cleanHtml}</div>
			</div>
			{/if}
			<div class="datemod col-xs-12 mrgn-tp-lg">
				<dl id="wb-dtmd">
					{if isNonnull($dateModified)}
					<dt>Date modified:&#32;</dt>
					<dd><time property="dateModified">{$dateModified|cleanHtml}</time></dd>
					{else}
					{if isNonnull($versionIdentifier)}
					<dt>Version:&#32;</dt>
					<dd>{$versionIdentifier|cleanHtml}</dd>
					{/if}
					{/if}
				</dl>
			</div>
		</div>
{/template}

/**
 * This is all of the html for the secondary menu.
 * @param sections Loops of the sections
 */
{template .secmenu}
<h2 id="wb-sec-h" class="wb-inv">Section menu</h2>
{foreach $section in $sections}
<section class="list-group menu list-unstyled">
	<h3>{if isNonnull($section.sectionLink)}<a href="{$section.sectionLink|escapeUri}">{/if}{$section.sectionName|cleanHtml}{if isNonnull($section.sectionLink)}</a>{/if}</h3>
	<ul class="list-group menu list-unstyled">
		{foreach $link in $section.menuLinks}
		{if isNonnull($link.subLinks)}
		<li>
			<a href="{$link.href|escapeUri}" class="list-group-item">{$link.text|cleanHtml}</a>
			<ul class="list-group menu list-unstyled">
				{foreach $sublink in $link.subLinks}
				<li><a href="{$sublink.subhref|escapeUri}" class="list-group-item">{$sublink.subtext|cleanHtml}</a></li>
				{/foreach}
			</ul>
		</li>
		{else}
		<li><a href="{$link.href|escapeUri}" class="list-group-item">{$link.text|cleanHtml}</a></li>
		{/if}
		{/foreach}
	</ul>
</section>
{/foreach}
{/template}

