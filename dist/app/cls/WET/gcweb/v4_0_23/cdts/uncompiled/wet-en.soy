/**
 * This is all of the html to be included before existing page content.
 * @param? topSecMenu Set to true if you want to use the secondary navigation.
 * @param? secure Used to determine if the site is secured or not.
 * @param? templateDomain The domain from which to get WET files.
 * @param cdnEnv Used to switch between qat, nonprod, and prod CDTS implementations.
 * @param appName Used to determine the name of the app.
 * @param? signOut Used to customize the link to sign out.
 * @param? signIn Used to customize the link to sign in.
 * @param? search Used to display search.
 * @param? siteMenu Used to display site menu.
 * @param? lngLinks Links to pages in other languages.
 * @param? customSearch Used to control the custom searches.
 * @param? menuPath Used to define the path to the menu that will be ajaxed in.
 * @param? breadcrumbs Used to set custom breadcrumbs.
 * @param? showPreContent Used to determine if the precontent space will be displayed.
 */
{template .appTop}
		<ul id="wb-tphp">
			<li class="wb-slc"><a class="wb-sl" href="#wb-cont">Skip to main content</a></li>
			<li class="wb-slc visible-sm visible-md visible-lg"><a class="wb-sl" href="#wb-info">Skip to "About this site"</a></li>
			{if $topSecMenu == true}
			<li class="wb-slc visible-md visible-lg"><a class="wb-sl" href="#wb-sec">Skip to section menu</a></li>
			{/if}
		</ul>
		<header role="banner" class="gc-app">
			<div id="wb-bnr" class="container">
				<div class="row">
					<div class="brand col-xs-8 col-sm-9 col-md-6">
						{if $secure != true}<a href="https://www.canada.ca/en.html">{/if}<object type="image/svg+xml" tabindex="-1" data="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='esdcqat'}https://cdn-canada.services.gc.qat/app/cls/WET{elseif $cdnEnv=='esdcnonprod'}https://s2tst-cdn-canada.sade-edap.prv/app/cls/WET{elseif $cdnEnv=='esdcprod'}https://ssl-templates.services.gc.ca/app/cls/WET{else}https://www.canada.ca/etc/designs/canada/cdts{/if}/gcweb/v4_0_23/assets/sig-blk-en.svg"></object><span class="wb-inv"> Government of Canada</span>{if $secure != true}</a>{/if}
					</div>
					<div class="visible-xxs hidden-print">
						<div class="col-xs-8 gc-app-name">
							<p class="h1">{if $secure == true}<i class="glyphicon glyphicon-lock mrgn-rght-md"></i>{/if}{$appName}</p>
						</div>
						<div class="col-xs-4 text-right visible-xxs">
							{if isNonnull($signOut)}
							{foreach $button in $signOut}
							{if isFirst($button)}
							<a href="{$button.href}" class="btn btn-primary btn-signoff">Sign out</a>
							{/if}
							{/foreach}
							{elseif isNonnull($signIn)}
							{foreach $button in $signIn}
							{if isFirst($button)}
							<a href="{$button.href}" class="btn btn-primary btn-signoff">Sign in</a>
							{/if}
							{/foreach}
							{else}
							{/if}
						</div>
					</div>
					<section class="wb-mb-links col-xs-4 col-sm-3 visible-sm visible-xs" id="wb-glb-mn">
						<h2>{if $search != false}Search and menu{if $siteMenu != false}s{/if}{else}Menu{if $siteMenu != false}s{/if}{/if}</h2>
						<ul class="list-inline text-right chvrn">
							<li><a href="#mb-pnl" title="{if $search != false}Search and menu{if $siteMenu != false}s{/if}{else}Menu{if $siteMenu != false}s{/if}{/if}" aria-controls="mb-pnl" class="overlay-lnk" role="button">{if $search != false}<span class="glyphicon glyphicon-search">{/if}<span class="glyphicon glyphicon-th-list"><span class="wb-inv">{if $search != false}Search and menu{if $siteMenu != false}s{/if}{else}Menu{if $siteMenu != false}s{/if}{/if}</span></span>{if $search != false}</span>{/if}</a></li>
						</ul>
						<div id="mb-pnl"></div>
					</section>
					{if isNonnull($lngLinks)}
					<section id="wb-lng" class="visible-md visible-lg text-right col-md-6">
						<h2 class="wb-inv">Language selection</h2>
						<div class="row">
							<div class="col-md-12">
								<ul class="list-inline margin-bottom-none">
									{foreach $link in $lngLinks}
									<li><a lang="{$link.lang}" href="{$link.href}">{$link.text}</a></li>
									{/foreach}
								</ul>
							</div>
						</div>
					</section>
					{/if}
				</div>
				<div class="clearfix"></div>
				<div class="row gc-app-br">
					<div class="hidden-xxs">
						<div class="col-xs-8">
							<p class="h1">{if $secure == true}<i class="glyphicon glyphicon-lock mrgn-rght-md"></i>{/if}{$appName}</p>
						</div>
						{if isNonnull($signOut)}
						<div class="col-xs-4 text-right visible-sm visible-xs">
							{foreach $button in $signOut}
							{if isFirst($button)}
							<a href="{$button.href}" class="btn btn-primary btn-signoff"><i class="glyphicon glyphicon-off"></i>Sign out</a>
							{/if}
							{/foreach}
						</div>
						{elseif isNonnull($signIn)}
						<div class="col-xs-4 text-right visible-sm visible-xs">
							{foreach $button in $signIn}
							{if isFirst($button)}
							<a href="{$button.href}" class="btn btn-primary btn-signoff"><i class="glyphicon glyphicon-off"></i>Sign in</a>
							{/if}
							{/foreach}
						</div>
						{else}
						{/if}
					</div>
					<div class="col-md-4 pull-right text-right">
						{if $search != false}
						{if isNonnull($customSearch)}
						{if $customSearch == 'xxx'}
						{/if}
						{else}
						<section id="wb-srch" class="text-right visible-md visible-lg">
							<h2>Search</h2>
							<form action="https://www.canada.ca/en/sr.html#wb-land" method="get" name="cse-search-box" role="search" class="form-inline">
								<div class="form-group">
									<label for="wb-srch-q" class="wb-inv">Search website</label>
									<input name="cdn" value="canada" type="hidden">
									<input name="st" value="s" type="hidden">
									<input name="num" value="10" type="hidden">
									<input name="langs" value="en" type="hidden">
									<input name="st1rt" value="1" type="hidden">
									<input name="s5bm3ts21rch" value="x" type="hidden">
									<input id="wb-srch-q" list="wb-srch-q-ac" class="wb-srch-q form-control" name="q" type="search" value="" size="27" maxlength="150" placeholder="Search Canada.ca">
									<input type="hidden" name="_charset_" value="UTF-8">
									<datalist id="wb-srch-q-ac">
										<!--[if lte IE 9]><select><![endif]-->
										<!--[if lte IE 9]></select><![endif]-->
									</datalist>
								</div>
								<div class="form-group submit">
									<button type="submit" id="wb-srch-sub" class="btn btn-primary btn-small" name="wb-srch-sub"><span class="glyphicon-search glyphicon"></span><span class="wb-inv">Search</span></button>
								</div>
							</form>
						</section>
						{/if}
						{/if}
					</div>
				</div>
			</div>
			<div id="wb-sm" class="visible-md visible-lg{if $siteMenu == false} wb-menu{/if}"{if $siteMenu == false} data-trgt="mb-pnl"{/if}>
				<div class="container">
					{if $siteMenu != false}
					<nav role="navigation" data-ajax-replace="{if isNonnull($menuPath)}{$menuPath}{else}https://ssl-templates.services.gc.ca/app/cls/WET/gcweb/v4_0_23/cdts/sitemenu-en.html{/if}" data-trgt="mb-pnl" class="wb-menu col-md-1{if isNonnull($signOut)}0{elseif isNonnull($signIn)}0{else}2{/if}" typeof="SiteNavigationElement">
						{if isNonnull($menuPath)}
						{else}
						<div class="nvbar">
							<h2 class="wb-inv">Topics menu</h2>
							<div class="row">
								<ul class="list-inline menu">
									<li><a href="http://www.esdc.gc.ca/en/jobs/index.page">Jobs</a></li>
									<li><a href="http://www.cic.gc.ca/english/index.asp">Immigration</a></li>
									<li><a href="https://travel.gc.ca/">Travel</a></li>
									<li><a href="https://www.canada.ca/en/services/business.html">Business</a></li>
									<li><a href="https://www.canada.ca/en/services/benefits.html">Benefits</a></li>
									<li><a href="https://www.canada.ca/en/services/health.html">Health</a></li>
									<li><a href="https://www.canada.ca/en/services/taxes.html">Taxes</a></li>
									<li><a href="https://www.canada.ca/en/services.html">More services</a></li>
								</ul>
							</div>
						</div>
						{/if}
					</nav>
					{else}
					<div class="col-md-1{if isNonnull($signOut)}0{elseif isNonnull($signIn)}0{else}2{/if}"></div>
					{/if}
					{if isNonnull($signOut)}
					<div class="text-right col-md-2 visible-md visible-lg">
						<div class="row">
							{foreach $button in $signOut}
							{if isFirst($button)}
							<p><a href="{$button.href}" class="btn btn-default btn btn-signoff"><i class="glyphicon glyphicon-off"></i>Sign out</a></p>
							{/if}
							{/foreach}
						</div>
					</div>
					{elseif isNonnull($signIn)}
					<div class="text-right col-md-2 visible-md visible-lg">
						<div class="row">
							{foreach $button in $signIn}
							{if isFirst($button)}
							<p><a href="{$button.href}" class="btn btn-default btn btn-signoff"><i class="glyphicon glyphicon-off"></i>Sign in</a></p>
							{/if}
							{/foreach}
						</div>
					</div>
					{else}
					{/if}
				</div>
			</div>
			{if isNonnull($breadcrumbs)}
			<nav role="navigation" id="wb-bc" property="breadcrumb">
				<h2>You are here:</h2>
				<div class="container">
					<div class="row">
						<ol class="breadcrumb">
							{foreach $item in $breadcrumbs}
							<li>{if isNonnull($item.acronym)}<abbr title="{$item.acronym}">{/if}{if isNonnull($item.href)}<a href="{$item.href}">{/if}{$item.title}{if isNonnull($item.href)}</a>{/if}{if isNonnull($item.acronym)}</abbr>{/if}</li>
							{/foreach}
						</ol>
					</div>
				</div>
			</nav>
			{/if}
		</header>
		{if $showPreContent != false}
		{/if}
{/template}

