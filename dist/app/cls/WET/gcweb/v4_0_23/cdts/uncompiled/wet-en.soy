		</div>
		<main role="main">
			<div class="sp-hb">
				<div class="sp-bx col-xs-12">
					<h1 property="name" class="wb-inv">Canada.ca</h1>
					<div class="row">
						<div class="col-xs-11 col-md-8">
							<object type="image/svg+xml" tabindex="-1" role="img" data="{if isNonnull($templateDomain)}{$templateDomain}{elseif $cdnEnv=='esdcqat'}https://cdn-canada.services.gc.qat/app/cls/WET{elseif $cdnEnv=='esdcnonprod'}https://s2tst-cdn-canada.sade-edap.prv/app/cls/WET{elseif $cdnEnv=='esdcprod'}https://ssl-templates.services.gc.ca/app/cls/WET{else}https://www.canada.ca/etc/designs/canada/cdts{/if}/gcweb/v4_0_23/assets/sig-spl.svg" width="283" aria-label="Government of Canada / Gouvernement du Canada"></object>
						</div>
					</div>
					<div class="row">
						<section class="col-xs-6 text-right">
							{if isNonnull($nameEng)}
							<h2 style="min-height:4.5em;">{$nameEng}</h2>
							{else}
							<h2 class="wb-inv">Government of Canada</h2>
							{/if}
							<p><a href="{$indexEng}" class="btn btn-primary">English</a></p>
						</section>
						<section class="col-xs-6" lang="fr">
							{if isNonnull($nameFra)}
							<h2 style="min-height:4.5em;">{$nameFra}</h2>
							{else}
							<h2 class="wb-inv">Gouvernement du Canada</h2>
							{/if}
							<p><a href="{$indexFra}" class="btn btn-primary">Français</a></p>
						</section>
					</div>
				</div>
				<div class="sp-bx-bt col-xs-12">
